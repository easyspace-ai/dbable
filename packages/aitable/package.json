{
  "name": "@easygrid/aitable",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Next-generation Airtable-like component for EasyGrid",
  "homepage": "https://github.com/your-username/easygrid",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "default": "./dist/style.css"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js",
      "require": "./dist/store/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    },
    "./grid/error-handling": {
      "types": "./dist/grid/error-handling/index.d.ts",
      "import": "./dist/grid/error-handling/index.js",
      "require": "./dist/grid/error-handling/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "kanban"
  ],
  "scripts": {
    "build": "vite build",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./node_modules/.cache ./.eslintcache",
    "dev": "vite build --watch",
    "dev:examples": "vite --config examples/vite.config.ts",
    "demo:yjs": "vite --config demo-yjs/vite.config.ts",
    "demo:yjs:build": "vite build --config demo-yjs/vite.config.ts",
    "build:examples": "vite build --config examples/vite.config.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "immer": "^10.0.4",
    "lodash": "4.17.21",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.545.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-textarea-autosize": "^8.5.9",
    "react-use": "^17.6.0",
    "ts-keycode-enum": "^1.0.6",
    "use-file-picker": "^2.1.4"
  },
  "peerDependencies": {
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.11.7",
    "@tanstack/react-virtual": "^3.2.0",
    "react": "^18.0.0",
    "react-day-picker": "^9.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.0",
    "zustand": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@easygrid/eslint-config": "workspace:*",
    "@easygrid/typescript-config": "workspace:*",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.11.7",
    "@tanstack/react-virtual": "^3.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "4.17.0",
    "@types/node": "20.9.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-hammerjs": "1.0.7",
    "@types/sharedb": "^3.3.10",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "7.0.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "react": "^18.0.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.0.0",
    "rimraf": "5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.0.0",
    "vitest": "2.1.5",
    "zustand": "4.5.2"
  }
}